<apex:page controller="DML_Insert_1">
    <apex:form id="one">
        <apex:pageBlock title="Create Account">
            <apex:pageBlockButtons >
                <apex:commandButton value="Create" action="{!create}"/>
                <apex:commandButton value="Cancel" action="{!cancel}" reRender="one"/>
            </apex:pageBlockButtons>
            <apex:pageBlockSection columns="1">
                <apex:pageBlockSectionItem > 
                    <apex:outputLabel value="Account Name"/>
                    <apex:inputText value="{!accName}"/>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem > 
                    <apex:outputLabel value="Account Phone"/>
                    <apex:inputText value="{!phone}"/>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem > 
                    <apex:outputLabel value="Account Industry"/>
                    <apex:inputText value="{!industry}"/>
                </apex:pageBlockSectionItem> 
                <apex:pageBlockSectionItem > 
                <apex:outputLabel value="Account Rating"/>
                <apex:inputText value="{!rating}"/>
                </apex:pageBlockSectionItem>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>